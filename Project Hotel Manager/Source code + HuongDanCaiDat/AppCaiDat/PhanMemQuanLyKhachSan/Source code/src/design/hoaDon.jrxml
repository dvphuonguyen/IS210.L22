	<subDataset name="data_room" uuid="4acfa2bb-e14a-411a-a776-79f45c8d6bde">
		<parameter name="room_hd" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select ngaynp, tenphong, round(ngaytptt - ngaynp + 1) as sl, giaphong, phuthu,
		 round(ngaytptt - ngaynp + 1)*giaphong*(1+phuthu) as tien_tung_phong
from hoadon hd join (   select ptp.maptp, ptp.tienphong, cptp.tenphong, cptp.ngaynp, cptp.ngaytptt, cptp.giaphong, cptp.phuthu
                        from phieuthuephong ptp join (  select ctptp.maptp, plp.tenphong, ctptp.ngaynp, ctptp.ngaytptt, plp.giaphong, ctptp.phuthu
                                                        from chitietptp ctptp join (    select p.maphong, p.tenphong, lp.giaphong
                                                                                        from phong p join loaiphong lp on p.malp = lp.malp
                                                                                    )  plp on plp.maphong = ctptp.maphong
                                                     ) cptp on cptp.maptp = ptp.maptp
                    ) tt_ptp on tt_ptp.maptp = hd.maptp
 where mahd = $P{room_hd}]]>
		</queryString>
		<field name="NGAYNP" class="java.sql.Timestamp"/>
		<field name="TENPHONG" class="java.lang.String"/>
		<field name="SL" class="java.math.BigDecimal"/>
		<field name="GIAPHONG" class="java.math.BigDecimal"/>
		<field name="PHUTHU" class="java.math.BigDecimal"/>
		<field name="TIEN_TUNG_PHONG" class="java.math.BigDecimal"/>
	</subDataset>
	<subDataset name="dichvu_room" uuid="0800d0ac-62f0-4e61-8a6c-373653ec489f">
		<parameter name="services_hd" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[select ngaysd, tendv, giadv, sldv, '0.00' as phuthu, giadv * sldv as tien_tung_dv
from hoadon hd join (   select pdv.mapdv, pdv.tiendv, cpdv.tendv, cpdv.sldv, cpdv.giadv, cpdv.ngaysd
                        from phieudichvu pdv join ( select ctpdv.mapdv, dv.tendv, ctpdv.sldv, dv.giadv, ctpdv.ngaysd
                                                    from chitietpdv ctpdv join dichvu dv on ctpdv.madv = dv.madv
                                                    ) cpdv on pdv.mapdv = cpdv.mapdv
                    ) tt_pdv on hd.mapdv = tt_pdv.mapdv
 where mahd = $P{services_hd}]]>
		</queryString>
		<field name="NGAYSD" class="java.util.Date"/>
		<field name="TENDV" class="java.lang.String"/>
		<field name="GIADV" class="java.lang.Integer"/>
		<field name="SLDV" class="java.lang.Integer"/>
		<field name="PHUTHU" class="java.lang.Float"/>
		<field name="TIEN_TUNG_DV" class="java.lang.Integer"/>
	</subDataset>
	<parameter name="MAHOADON" class="java.lang.Integer"/>
	<queryString language="plsql">
		<![CDATA[select  mahd, tongtien, tenkh, ngaytt, hd.maptp, hd.mapdv, case
        when hd.maptp is null then tien_dv
        when hd.mapdv is null then tien_phong
        ELSE (tien_dv + tien_phong)
        end as tong_tien_tuc_te
from hoadon hd join khachhang kh on hd.makh = kh.makh
                left join (   select maptp, SUM(round(ngaytptt - ngaynp + 1)*giaphong*(1+phuthu)) as tien_phong
                        from chitietptp ctptp join (    select p.maphong, p.tenphong, lp.giaphong
                                                        from phong p join loaiphong lp on p.malp = lp.malp
                                                        )  plp on plp.maphong = ctptp.maphong
                        group by maptp
                    ) cptp on cptp.maptp = hd.maptp
               left join (   select ctpdv.mapdv, SUM(giadv * sldv) as tien_dv
                         from chitietpdv ctpdv join dichvu dv on ctpdv.madv = dv.madv
                         group by mapdv
                    ) cpdv on hd.mapdv = cpdv.mapdv
where mahd =  $P{MAHOADON}]]>
	</queryString>
	<field name="MAHD" class="java.math.BigDecimal"/>
	<field name="TONGTIEN" class="java.lang.Integer"/>
	<field name="TENKH" class="java.lang.String"/>
	<field name="NGAYTT" class="java.sql.Timestamp"/>
	<field name="MAPTP" class="java.math.BigDecimal"/>
	<field name="MAPDV" class="java.math.BigDecimal"/>
	<field name="TONG_TIEN_TUC_TE" class="java.lang.Integer"/>
	<group name="group_mahd">
		<groupExpression><![CDATA[$F{MAHD}]]></groupExpression>
	</group>
